<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane id="borderClassement" prefHeight="200.0" prefWidth="200.0" stylesheets="@style_app.css" xmlns="http://javafx.com/javafx/20.0.2" xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <left>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Text smooth="false" strokeType="OUTSIDE" strokeWidth="0.0" text="Pseudo :" underline="true">
               <font>
                  <Font size="16.0" />
               </font></Text>
            <Text fx:id="LabelPseudoParam" strokeType="OUTSIDE" strokeWidth="0.0" text="affiche pseudo" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="mail" underline="true">
               <font>
                  <Font size="16.0" />
               </font></Text>
            <Text fx:id="LabelPseudoMail" strokeType="OUTSIDE" strokeWidth="0.0" text="affiche mail" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Role" underline="true">
               <font>
                  <Font size="16.0" />
               </font></Text>
            <Text fx:id="LabelPseudoRole" strokeType="OUTSIDE" strokeWidth="0.0" text="affiche role" />
            <Button fx:id="BoutonSaveParametre" mnemonicParsing="false" text="save" visible="false" />
            <Button fx:id="BoutonDeconnecterParametre" mnemonicParsing="false" text="Deconnecter" />
         </children>
      </VBox>
   </left>
   <center>
      <BorderPane fx:id="BorderPaneParamTableau" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <top>
            <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <HBox fx:id="dragAndDropCsv" prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: red;" visible="false" />
                  <Button fx:id="FileChooser" mnemonicParsing="false" text="Button" visible="false" />
               </children>
            </HBox>
         </top>
      </BorderPane>
   </center>
</BorderPane>
